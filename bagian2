var item = prompt('Masukkan Nama Makanan/Minuman \n(cth: kentang, ikan, air, pizza, softdrink)');

switch(item)
{
	case 'susu' :
	case 'ikan' : 
	case 'air' :
	alert('Makanan/Minuman Sehat');
	break;
	case 'pizza' :
	case 'softdrink' : 
	alert('Makanan/Minuman Tidak Sehat');
	break;
	default :
	alert('Nama Makanan/Minuman Tidak Tersedia Dalam Daftar');
	break;
}
